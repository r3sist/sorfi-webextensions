{
  "name": "Sorozat figyel\u0151 felirat értesítő",
  "version": "3.0.1",
  "manifest_version": 2,
  "description": "Sorozat figyelő felirat értesítő",
  "homepage_url": "https://sorfi.org",
  "icons":
  {
    "16": "assets/sorfi-webextensions-icon16.png",
    "19": "assets/sorfi-webextensions-icon19.png",
    "48": "assets/sorfi-webextensions-icon48.png",
    "64": "assets/sorfi-webextensions-icon64.png",
    "128": "assets/sorfi-webextensions-icon128.png"
  },
  "default_locale": "hu",
  "background":
  {
    "page": "sf-ch-bg.html",
    "persistent": true
  },
  "options_page": "sf-ch-opt.html",
  "permissions":
  [
    "contextMenus",
    "tabs",
    "https://sorfi.org/*",
    "http://sorfi.org/*",
    "notifications",
    "storage"
  ],
  "browser_action":
  {
    "default_popup": "sf-ch.html",
    "default_icon": "icons/sf-ch-icon19.png",
    "default_title": "Sorozat figyel\u0151"
  },
  "omnibox": {
    "keyword": "sf"
  }
}